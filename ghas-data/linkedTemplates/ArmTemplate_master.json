{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ghas-data"
		},
		"data_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'data'"
		},
		"github_rest_api_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.github.com"
		},
		"vault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://data-pipeline-vault.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ghas-data_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"data_connectionString": {
						"value": "[parameters('data_connectionString')]"
					},
					"vault_properties_typeProperties_baseUrl": {
						"value": "[parameters('vault_properties_typeProperties_baseUrl')]"
					},
					"github_rest_api_properties_typeProperties_url": {
						"value": "[parameters('github_rest_api_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}